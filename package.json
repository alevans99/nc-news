{
  "name": "be-nc-news",
  "version": "1.0.0",
  "description": "bc-nc-news",
  "main": "index.js",
  "scripts": {
    "setup-dbs": "psql -f ./db/setup.sql",
    "seed": "node ./db/seeds/run-seed.js",
    "test-seed": "node ./db/seeds/run-test-seed.js",
    "test": "jest",
    "seed:prod": "NODE_ENV=production DATABASE_URL=$(heroku config:get DATABASE_URL) npm run seed",
    "start": "node listen.js",
    "test-insomnia": "NODE_ENV=test npm run test-seed && node listen.js",
    "test-insomnia-windows": "SET NODE_ENV=test&& npm run test-seed && node listen.js",
    "seed:prod-windows": "SET NODE_ENV=production&& SET DATABASE_URL=$(heroku config:get DATABASE_URL)&& npm run seed",
    "seed:railway": "SET NODE_ENV=railway&& npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northcoders/be-nc-news.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/northcoders/be-nc-news/issues"
  },
  "homepage": "https://github.com/northcoders/be-nc-news#readme",
  "devDependencies": {
    "jest": "^27.3.1",
    "jest-sorted": "^1.0.12",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "pg-format": "^1.0.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-sorted"
    ]
  }
}
